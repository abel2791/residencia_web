-webkit-box-shadow: 0px 0px 10px white;
}

article{
width:55%;
/*background:white;(255,255,255,0)*/
/*background:azul claro act.*/
background:rgba(255,255,255,1);
margin:10px;
height:100px;
display:table;
border: 1px solid white;
border-radius:0px 25px 0px 25px;
-webkit-border-radius:0px 25px 0px 25px;
-moz-border-radius:0px 25px 0px 25px;
-o-border-radius:0px 25px 0px 25px;

box-shadow: 3px 3px 10px black;
-webkit-box-shadow: 3px 3px 10px black;
-moz-box-shadow:3px 3px 10px black;

padding:10px 10px 10px 150px;
opacity:0.5;
transition: opacity 1s, box-shadow 1s, transform 1s, background 1s;
-webkit-transition: opacity 1s, -webkit-box-shadow 1s, -webkit-transform 1s, background 1s;
transform:scale(1,1);
-webkit-transform:scale(1,1);
-moz-transform:scale(1,1);
-o-transform:scale(1,1);
}

article:hover{
background:rgba(255,255,255,1);
opacity:1;
transform:scale(1.1,1.1);
-webkit-transform:scale(1.1,1.1);
-moz-transform:scale(1.1,1.1);/**/
-o-transform:scale(1.1,1.1);/**/
}

time{
font-family:ubuntuRegular;
font-size:10px;
}



article:hover{
box-shadow:0px 0px 25px white;
-webkit-box-shadow:0px 0px 25px white;
-moz-box-shadow:0px 0px 25px white;
}

aside{
float:right;
display:inline-table;
width:20%;

margin:10px;
padding:10px;
border: 1px solid white;
border-radius:0px 25px 0px 25px;
